{
    "files.exclude": {
        "**/*.pyc": {"when": "$(basename).py"},
        "**/*.pytest_cache": true,
        "**/__pycache__": true
    },
    "python.analysis.extraPaths": [
        ".ignore/site-packages",
        "src", "test"
    ],
    "python.autoComplete.extraPaths": [
        ".ignore/site-packages",
        "src", "test"
    ]
}
